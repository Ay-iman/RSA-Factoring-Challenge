#include <stdio.h>
#include <stdlib.h>
#include <math.h>

int is_prime(int n) {
    if (n <= 1) return 0;
    if (n <= 3) return 1;
    if (n % 2 == 0 || n % 3 == 0) return 0;
    for (int i = 5; i * i <= n; i += 6) {
        if (n % i == 0 || n % (i + 2) == 0) {
            return 0;
        }
    }
    return 1;
}

void find_primes(int n, int *p, int *q) {
    int i, j;
    for (i = 2; i < n; i++) {
        if (n % i == 0 && is_prime(i)) {
            *p = i;
            for (j = i + 1; j < n; j++) {
                if (n % j == 0 && is_prime(j)) {
                    *q = j;
                    return;
                }
            }
        }
    }
}

int main(int argc, char **argv) {
    if (argc != 2) {
        printf("Usage: %s <number>\n", argv[0]);
        return 1;
    }
    int n = atoi(argv[1]);
    int p, q;
    find_primes(n, &p, &q);
    printf("%d=%d*%d\n", n, p, q);
    return 0;
}

